---
slug: profile
method: put
---
{% liquid
  include 'lib/must_be_authenticated'

  function_rc object = 'lib/commands/users/update', object: context.params.user
  if object.valid
    include 'redirect_to', url: '/profile?a=b'
  else
    graphql presigned_url = 'users/photos/presign' | dig: 'presigned'
    include 'theme/simple/profile/edit', object: user, presigned_url: presigned_url
  endif
%}
